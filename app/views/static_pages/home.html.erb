<% if current_user %>
<div class="form-group">
    <h4> <center> First Name </center> </h4>
    <center> <%= current_user.first_name %> </center>
</div>

<div class="form-group">
    <h4> <center> Last Name </center> </h4>
    <center> <%= current_user.last_name %> </center>
</div>

<div class="form-group">
    <h4> <center> Email </center> </h4>
    <center> <%= current_user.email %> </center>
</div>

<div class="form-group">
    <h4> <center> Date of Birth </center> </h4>
    <center> <%= current_user.date_of_birth %> </center>
</div>


<% end %>